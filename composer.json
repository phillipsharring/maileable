{
  "name": "philsown/maileable",
  "description": "Maileable, a library for adding filters to Laravel Mailable messages",
  "keywords": ["library", "laravel", "mail", "email", "mailables", "swift", "swift_message"],
  "license": "MIT",
  "homepage": "https://phillipharrington.com",
  "support": {
    "issues": "https://github.com/philsown/maileable/issues",
    "source": "https://github.com/philsown/maileable"
  },
  "authors": [
    {
      "name": "Phillip Harrington",
      "email": "phillip@phillipharrington.com"
    }
  ],
  "require": {
    "php": ">=5.6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Maileable\\": "src/Maileable/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Maileable\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
